  "description": "NodeAuthBase-JS is an authentication library designed for Node.js, offering streamlined implementation of authentication functionalities within Express applications.",
  "main": "src/server.js",
  "bin": {
    "node-auth-base-js": "src/bin/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hiral-makwana/NodeAuthBase-JS.git"
  },
  "scripts": {
